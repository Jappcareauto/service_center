// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Appointment {
  date DateTime
  locationType String
  note String
  serviceId String
  vehicleId String
  status String
  id Int @id @unique
  uid String
  createdBy String
  updatedBy String
  createdAt DateTime @default(now())
  updatedAt DateTime
}